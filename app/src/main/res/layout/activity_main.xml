<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/clickFontStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000"
            android:gravity="center"
            android:padding="20dp"
            android:text="点击跳转到字体test activity"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickTabLayoutFloatTop"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#002"
            android:gravity="center"
            android:padding="20dp"
            android:text="tab悬浮在最上方,AppbarLayout的简单用法"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickSnapHelpTest"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#003"
            android:gravity="center"
            android:padding="20dp"
            android:text="SnapHelp测试"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickKuGouSlidingMenu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#004"
            android:gravity="center"
            android:padding="20dp"
            android:text="仿酷狗侧边栏效果"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickQq6SlidingMenu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#005"
            android:gravity="center"
            android:padding="20dp"
            android:text="仿qq6.0侧边栏效果"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickCarHomeItem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#006"
            android:gravity="center"
            android:padding="20dp"
            android:text="汽车之家折叠列表"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/click9LockPalaces"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#007"
            android:gravity="center"
            android:padding="20dp"
            android:text="9宫格解锁样式"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickNotchLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#008"
            android:gravity="center"
            android:padding="20dp"
            android:text="自定义view缺口线"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickStatusBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#009"
            android:gravity="center"
            android:padding="20dp"
            android:text="状态栏测试"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickCustomBehavior"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00a"
            android:gravity="center"
            android:padding="20dp"
            android:text="自定义behavior"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickCarNumberKeyboard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00b"
            android:gravity="center"
            android:padding="20dp"
            android:text="自定义车牌号键盘"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/clickEqualScaleCouponViewActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00c"
            android:gravity="center"
            android:padding="20dp"
            android:text="优惠券view 按比例缩放，形成在任意手机上效果一致"
            android:textColor="@android:color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/clickLoadingAnimationActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00d"
            android:gravity="center"
            android:padding="20dp"
            android:text="加载动画00"
            android:textColor="@android:color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/clickRatingBarActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00e"
            android:gravity="center"
            android:padding="20dp"
            android:text="自定义评价星星"
            android:textColor="@android:color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/clickBezierActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00f"
            android:gravity="center"
            android:padding="20dp"
            android:text="贝塞尔曲线学习"
            android:textColor="@android:color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/clickParallaxAnimatorActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#02f"
            android:gravity="center"
            android:padding="20dp"
            android:text="视差动画学习"
            android:textColor="@android:color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/clickWXBookActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#03f"
            android:gravity="center"
            android:padding="20dp"
            android:text="仿微信通讯录"
            android:textColor="@android:color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/clickHookViewActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#04f"
            android:gravity="center"
            android:padding="20dp"
            android:text="HookViewTest"
            android:textColor="@android:color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/clickMyEditTextActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#05f"
            android:gravity="center"
            android:padding="20dp"
            android:text="自定义输入框"
            android:textColor="@android:color/white"
            android:textSize="12dp" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>